@Article{Durbin2017,
  author          = {Durbin, Anna and Brown, Hilary K. and Antoniou, Tony and Sirotich, Frank and Bansal, Symron and Heifetz, Marina and Roesslein, Kay and Lunsky, Yona},
  journal         = {AIDS and behavior},
  title           = {Mental Health Disorders and Publicly Funded Service Use by HIV Positive Individuals: A Population-Based Cross-Sectional Study in Ontario, Canada.},
