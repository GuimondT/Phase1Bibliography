  issue           = {6},
  keywords        = {Adolescent; Adult; Antidepressive Agents, therapeutic use; Depression, drug therapy, epidemiology, etiology, psychology; Female; HIV Infections, complications, psychology, virology; HIV-1, isolation & purification; Health Services Needs and Demand; Humans; Incidence; Male; Mental Health Services, standards; Middle Aged; Ontario, epidemiology; Prospective Studies; Psychiatric Status Rating Scales; Risk Factors; Socioeconomic Factors; Young Adult},
