@Article{McKinnon2020,
  author          = {McKinnon, Karen and Satriano, James and Alves-Bradford, Jean-Marie and Erby, Whitney and Jaafar, Fatima J. and Simonen, Elizabeth H. and Gozzo, Izabella S. and Robles Huang, Amy N. and Sposito, Jonah S. and Tao, Ziyi and Zakoian, Martin J. and Zurita McKinnon, Alma and Cournos, Francine},
  journal         = {Psychiatric services (Washington, D.C.)},
  title           = {Three Decades and Counting: HIV Service Provision in Outpatient Mental Health Settings.},
  year            = {2020},
  issn            = {1557-9700},
  month           = mar,
  pages           = {appips201900415},
  abstract        = {People with serious mental illness in the United States have higher human immunodeficiency virus (HIV) infection rates than the general U.S. population. This study aimed to assess delivery of HIV services in New York State's outpatient mental health programs. Greater access would enhance efforts to improve HIV prevention and care outcomes. The authors surveyed directors of licensed outpatient mental health care programs statewide to investigate their HIV service delivery. Data were compared with surveys conducted in 1997 and 2004 in order to examine differences in services between geographic regions and time periods. Outpatient mental health programs have improved in the volume and range of HIV services offered, but their provision of preexposure prophylaxis, condoms, HIV testing, and HIV antiretroviral treatment monitoring has lagged. New York's initiative to end the HIV epidemic is not optimized to reach people with serious mental illness in settings designed for their care.},
