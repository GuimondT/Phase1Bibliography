  issn-linking    = {0884-8734},
  issue           = {7},
  keywords        = {Adult; Computer Communication Networks; Decision Support Techniques; Depression, therapy; Female; Guideline Adherence; Humans; Male; Middle Aged; Practice Guidelines as Topic; Primary Health Care; Treatment Outcome},
